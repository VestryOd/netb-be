{
  "name": "netb-be",
  "version": "1.0.0",
  "description": "node-express-mongo-db project with typescript",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production nodemon app/index.ts",
    "start:dev": "nodemon --files app/index.ts",
    "prettier": "find app -type f -name '*.ts' | xargs prettier --write",
    "lint": "eslint --ext .ts .",
    "commit-hook": "git add .",
    "before:module_alias": "which link-module-alias && link-module-alias clean || echo",
    "after:module_alias": "link-module-alias"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VestryOd/netb-be.git"
  },
  "keywords": [
    "express",
    "nide.js",
    "mongoose",
    "mongodb",
    "typescript"
  ],
  "author": "VestryOd",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/VestryOd/netb-be/issues"
  },
  "pre-commit": [
    "prettier",
    "lint",
    "commit-hook"
  ],
  "homepage": "https://github.com/VestryOd/netb-be#readme",
  "_moduleAliases": {
    "@": "./app"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.131.0",
    "@types/http-errors": "^1.8.2",
    "aws-sdk": "^2.1176.0",
    "axios": "^0.27.2",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.0",
    "compose-middleware": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-fileupload": "^1.4.0",
    "express-http-context": "^1.2.4",
    "express-joi-validation": "^5.0.1",
    "express-winston": "^4.2.0",
    "form-data": "^4.0.0",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.2.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "module-alias": "^2.2.2",
    "mongoose": "^6.3.5",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "swagger-ui-express": "^4.6.0",
    "uuid": "^8.3.2",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.2.2",
    "@types/express-winston": "^4.0.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.0.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.11.0",
    "eslint-plugin-jsdoc": "^38.0.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "husky": "^8.0.1",
    "link-module-alias": "^1.2.0",
    "nodemon": "^2.0.16",
    "pre-commit": "^1.2.2",
    "prettier": "^2.6.2",
    "ts-node": "^10.8.0",
    "tslint": "^6.1.3",
    "typescript": "^4.7.2"
  }
